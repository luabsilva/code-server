{
  "name": "test",
  "version": "1.0.0",
  "description": "code-server tests",
  "main": "index.ts",
  "license": "MIT",
  "scripts": {
    "postinstall": "../node_modules/.bin/ts-node ../scripts/install-packages.ts",
    "test": "npx jest --verbose src"
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/puppeteer": "^1.12.3",
    "@types/xml2js": "^0.4.3",
    "jest": "^24.5.0",
    "ps-list": "^6.2.0",
    "puppeteer": "^1.13.0",
    "ts-jest": "^24.0.0",
    "xml2js": "^0.4.19"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "setupFiles": [
      "<rootDir>/../scripts/test-setup.js"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/logger/"
    ],
    "testRegex": ".*\\.test\\.tsx?"
  }
}